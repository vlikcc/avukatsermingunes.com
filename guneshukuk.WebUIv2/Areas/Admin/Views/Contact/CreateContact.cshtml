@using guneshukuk.WebUIv2.Areas.Admin.Models.Dtos.ContactDtos
@model CreateContactDto
@{
    ViewData["Title"] = "CreateContact";
    Layout = "~/Areas/Admin/Views/AdminLayout/Index.cshtml";
}


<div class="content">
	<div class="container-fluid">
		<h4 class="page-title">İLETİŞİM BİLGİ GİRİŞİ</h4>
		<div class="row">
			<div class="col-md-12">
				<form method="post">
					<div class="card">


						<div class="form-group">
							<label for="Location">İletişim Adresi Giriniz</label>
							<input type="text" class="form-control" id="Location" name="Location" asp-for="Location">
						</div>
						<div class="form-group">
							<label for="Phone">Telefon Numarası Giriniz</label>
							<input type="text" class="form-control" id="Phone" name="Phone" asp-for="Phone">
						</div>
						<div class="card-body">
							<div class="form-group">
								<label for="Email">E-Posta Adresi Giriniz</label>
								<input type="text" class="form-control" id="Email" name="Email" asp-for="Email">
							</div>
						</div>
						<div class="card-action">
							<button class="btn btn-primary">İletişim Bilgisi Ekle</button>

						</div>
					</div>
				</form>
			</div>


		</div>
	</div>

